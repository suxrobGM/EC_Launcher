<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:local="clr-namespace:EC_Launcher"
                    xmlns:v="clr-namespace:System;assembly=mscorlib">
    <!--Main Window-->
    <v:String x:Key="m_OpenGame">Запустить игру</v:String>
    <v:String x:Key="m_CheckUpdate">Обновления</v:String>
    <v:String x:Key="m_ReportBug">Сообщить об ошибке</v:String>
    <v:String x:Key="m_Donate">Donate!</v:String>
    <v:String x:Key="m_Settings">Настройки</v:String>
